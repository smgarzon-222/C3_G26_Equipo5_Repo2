@page
@model Torneo.App.Frontend.Pages.Municipios.IndexModel
@{
}
<h2>
  LISTADO 
  <small class="text-muted">MUNICIPIOS</small>
</h2>
<table class="table table-borderlees table-sm table-hover">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nombre</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var municipio in Model.municipios)
        {
            <tr>
                <th scope="row">@municipio.Id</th>
                <td>@municipio.Nombre</td>
            </tr>
        }
        
    </tbody>
</table>
